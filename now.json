{
  "version": 2,
  "name": "jjwrites",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "/(.*\\.(js|json|css|ico|png)$)",
      "dest": "/$1",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" }
    }
  ],
  "alias": "jjwrites",
  "github": {
    "silent": true
  },
  "builds": [
    { "use": "@now/static-build", "src": "package.json", "config": { "distDir": "public" } }
  ]
}
